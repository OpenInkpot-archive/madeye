msgid "index"
msgstr ""
"<h1>MadEye: Help</h1>\n"
"\n"
"<br>\n"
"1) Controls\n"
"<br>\n"
"2) About"

msgid "madeye1"
msgstr ""
"<h2>Controls:</h2>\n"
"\n"
"<p>\"<b>0</b>\" - show help</p>\n"
"<p>\"<b>1</b>\"/\"<b>2</b>\" - change brightness of the image</p>\n"
"<p>\"<b>3</b>\"/\"<b>4</b>\" - change contrast of the image</p>\n"
"<p>\"<b>5</b>\" - change image viewing mode</p>\n"
"<p>\"<b>M</b>\" or \"<b>7</b>\"- toggle between full-screen and windowed modes</p>\n"
"<p>\"<b>Left</b>\"/\"<b>Right</b>\" и \"<b>Wheel</b>\"  - navigation berween images</p>\n"
"<p>\"<b>MENU</b>\" - exit to Main menu</p>\n"
"<p>\"<b>C</b>\"/\"<b>↶</b>\" - exit to previous menu</p>\n"
"<p>\"<b>Rotate</b>\" - change page orientation (vertical/ horizontal)</p>\n"
"<p>\"<b>Play</b>\"/\"<b>Stop</b>\" - open audio player</p>"

msgid "madeye2"
msgstr ""
"<h2>About:</h2>\n"
"\n"
"<p>Madeye – application for viewing images. Supported formats: jpeg, gif, png, xpm.</p>"

